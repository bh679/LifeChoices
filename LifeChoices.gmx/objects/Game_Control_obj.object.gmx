            <string>//Brennan Hatton  3/20/15
//in game level
if (room = Combat_room) //TODO replace with check for combat room, if we have mutliple rooms
{
    //for all characters
    for(i = 0; i &lt; global.totalChars; i++)
    {
        //check characters is alive this round
        if global.charAlive[i] == true
        {
            //this characters starts at 
            var charX, charY;
            //create at spawn point with a range
            charX = spawnX + spawnRange/2 - random(spawnRange/2) 
            charY = spawnY + spawnRange/2 - random(spawnRange/2)
            
            global.charRef[i] = instance_create(charX,charY,Char_obj) //save the characters reference
            
            global.charRef[i].charName = global.charName[i]//name the characters locally
            global.charRef[i].charID = i;//so character knows his own ID in the array
            global.charRef[i].charColor = global.charColor[i]//set his color
        }
    }
}
</string>
